{
    "name": "b2pweb/bdf-collections",
    "description": "Collections implementations with java 8 stream like api.",
    "type": "library",
    "keywords": [
        "collections", "functional", "java stream", "hashtable", "hashset"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Quatrevieux"
        }
    ],
    "autoload": {
        "psr-4": {
            "Bdf\\Collection\\": "src"
        }
    },
    "require": {
        "php": "~7.1 || ~8.0.0 || ~8.1.0 || ~8.2.0 || ~8.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0|~8.0",
        "phpbench/phpbench": "~0.0|~1.0",
        "vimeo/psalm": "~4.15"
    },
    "scripts": {
        "tests": "phpunit",
        "tests-with-coverage": "phpunit --coverage-clover coverage.xml",
        "psalm": "psalm --shepherd --output-format=github",
        "bc-break": "roave-backward-compatibility-check"
    }
}
